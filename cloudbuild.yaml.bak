steps:
- name: "ubuntu"
  args: ["bash", "./cloud_env.sh"]  # your script file
  env:
    - "USERNAME=${_USERNAME}"
    - "PASSWORD=${_PASSWORD}"
    - "CONNECTION_STRING=${_CONNECTION_STRING}"
- name: node
  entrypoint: npm
  args: ["install" , "--only=production"]
- name: "gcr.io/cloud-builders/gcloud"
  args: ["app", "deploy"]
timeout: "1600s"
options:
  logging: CLOUD_LOGGING_ONLY

